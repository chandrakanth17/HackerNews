{
  "name": "react-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack:client": "webpack --config webpack/webpack.client.js --watch",
    "webpack:server": "webpack --config webpack/webpack.server.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "dev": "npm-run-all --parallel webpack:*",
    "build": "webpack --config webpack/webpack.client.js && webpack --config webpack/webpack.server.js",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx,json}'"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.0",
    "react-redux": "7.1.1",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^3.1.0",
    "lint-staged": "^10.0.9",
    "loadash": "^1.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
