{
  "name": "react-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack:client": "webpack --config webpack/webpack.client.js -p --watch ",
    "webpack:server": "webpack --config webpack/webpack.server.js -p --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "start": "node build/bundle.js",
    "dev": "npm-run-all --parallel webpack:*",
    "build": "webpack --config webpack/webpack.client.js && webpack --config webpack/webpack.server.js",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx,json}'",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.2.1",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "express": "^4.17.1",
    "jest": "^25.4.0",
    "moment": "^2.24.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "body-parser": "^1.19.0",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.1.0"
  },
  "devDependencies": {
    "babel-jest": "^25.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": "13.5.0",
    "npm": "6.14.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  }
}
